program Test {
	i: int
	
	void Foo() {
		a, b: int
		max: int
		read a 
		read b
		if a > b{
		max = a
		}else{ 
		max = b
		}
		write max
	}

	void SumUp() {
		sum: int
		sum = 0
		while i > 0 { 
		sum = sum + i 
		i = i - 1 
		}write sum
	}

	void Main() {
		read i
		while i > 0 {
			SumUp()
			read i
		}
	}
